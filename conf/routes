# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

POST     /questionnaire                                io.perfix.common.PerfixController.questionnaire(storeName: String)
POST     /questionnaire/:id                            io.perfix.common.PerfixController.startExperiment(id: Int)
GET      /questionnaire/:id/question                   io.perfix.common.PerfixController.questions(id: Int)
POST     /questionnaire/:id/question                   io.perfix.common.PerfixController.submitQuestion(id: Int)

POST     /execute                                      io.perfix.common.PerfixController.executeExperiment(storeName: String)
