# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

POST     /questionnaire                                io.perfix.controller.PerfixController.questionnaire(storeName: String)
POST     /questionnaire/:id                            io.perfix.controller.PerfixController.startExperiment(id: Int)
GET      /questionnaire/:id/question                   io.perfix.controller.PerfixController.questions(id: Int)
POST     /questionnaire/:id/question                   io.perfix.controller.PerfixController.submitQuestion(id: Int)
